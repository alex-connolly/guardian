contract Ballot {

    class Voter {
        weight uint
        voted bool
        delegate address
        vote uint
    }

    class Proposal {
        name bytes32
        voteCount uint
    }

    external chairperson address
    external voters map[address]Voter
    external proposals [Proposal]

    constructor(proposalNames [bytes32]){

        chairperson = msg.sender
        voters[chairperson].weight = 1

    }



}
